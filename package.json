{
  "name": "fetch-mock-fixtures",
  "version": "2.0.0",
  "description": "Fixtures based mocker for fetch API with BDD style configuration",
  "main": "dist/index.min.js",
  "scripts": {
    "build": "webpack --config webpack.conf/build.js -p",
    "test": "cross-env BABEL_ENV=test karma start --single-run --browsers ChromeHeadless && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test:watch": "cross-env BABEL_ENV=test karma start --browsers ChromeHeadless  --log-level warn",
    "test:all": "cross-env BABEL_ENV=test karma start --single-run --log-level error"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "json-loader": "^0.5.7",
    "karma": "^4.0.0",
    "karma-babel-preprocessor": "^8.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.0-rc.6",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "pouchdb": "^7.0.0",
    "semantic-release": "^15.13.3",
    "sinon": "^7.3.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.2.0",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liqueurdetoile/fetch-mock-fixtures.git"
  },
  "keywords": [
    "fetch",
    "mock",
    "fixtures",
    "bdd"
  ],
  "author": "Liqueur de Toile",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liqueurdetoile/fetch-mock-fixtures/issues"
  },
  "homepage": "https://github.com/liqueurdetoile/fetch-mock-fixtures",
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "path-to-regexp": "^3.0.0",
    "url-parse": "^1.4.4"
  },
  "peerDependencies": {
    "sinon": "^7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
