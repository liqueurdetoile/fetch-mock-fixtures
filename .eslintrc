{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },

  "globals": {
    "document": false,
    "escape": false,
    "navigator": false,
    "unescape": false,
    "window": false,
    "describe": true,
    "before": true,
    "after": true,
    "beforeEach": true,
    "afterEach": true,
    "it": true,
    "expect": true,
    "sinon": true,
    "_capitalize": true,
    "_has": true,
    "_get": true,
    "_set": true,
    "_unset": true,
    "_cloneDeep": true,
    "_isEqual": true,
    "_debounce": true,
    "_throttle": true,
    "_upperFirst": true,
    "_unionBy": true,
    "UNITESTING": true
  },

  "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },

  "extends": [
    "eslint:recommended"
  ],

  "rules" : {
    "indent": "off",
    "keyword-spacing": [2, {"before": true, "after": true}]
  }
}
